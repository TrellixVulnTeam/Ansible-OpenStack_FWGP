#!/bin/sh

set -e

if [ "$1" = "remove" ] ; then
	update-alternatives --remove jsonschema /usr/bin/python2-jsonschema
fi


# Automatically added by dhpython:
if which pyclean >/dev/null 2>&1; then
	pyclean -p python-jsonschema 
else
	dpkg -L python-jsonschema | grep \.py$ | while read file
	do
		rm -f "${file}"[co] >/dev/null
  	done
fi

# End automatically added section


exit 0
