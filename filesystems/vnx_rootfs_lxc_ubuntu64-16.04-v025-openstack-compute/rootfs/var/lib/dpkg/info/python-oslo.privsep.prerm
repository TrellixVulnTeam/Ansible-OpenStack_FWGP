#!/bin/sh

set -e

if [ "$1" = "remove" ] ; then
	update-alternatives --remove privsep-helper /usr/bin/python2-privsep-helper
fi


# Automatically added by dh_python2:
if which pyclean >/dev/null 2>&1; then
	pyclean -p python-oslo.privsep 
else
	dpkg -L python-oslo.privsep | grep \.py$ | while read file
	do
		rm -f "${file}"[co] >/dev/null
  	done
fi

# End automatically added section


exit 0
